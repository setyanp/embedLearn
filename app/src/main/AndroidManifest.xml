<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.learn.embed">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.android.learn.embed.MainActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.android.learn.embed.Launcher"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.android.learn.embed.Materi"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.android.learn.embed.IotPage"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.android.learn.embed.RoboticPage"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.android.learn.embed.IpPage"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.android.learn.embed.videoMain"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.android.learn.embed.videoPage1"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.android.learn.embed.videoPage2"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.android.learn.embed.videoPage3"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.android.learn.embed.QuizPage"
            android:screenOrientation="portrait" >
        </activity>
    </application>

</manifest>